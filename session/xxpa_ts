let SessionLoad = 1
let s:so_save = &g:so | let s:siso_save = &g:siso | setg so=0 siso=0 | setl so=-1 siso=-1
let v:this_session=expand("<sfile>:p")
let Illuminate_highlightPriority =  -1 
let Illuminate_highlightUnderCursor =  1 
let Illuminate_delay =  0 
silent only
silent tabonly
cd /mnt/d/Development
if expand('%') == '' && !&modified && line('$') <= 1 && getline(1) == ''
  let s:wipebuf = bufnr('%')
endif
set shortmess=aoO
argglobal
%argdel
edit xxpa_ts/ebs_xxpa/sql/c4240.package.XXPA_TS_HEADER_PKG.sql
argglobal
balt xxpa_ts/ebs_xxpa/sql/c4042.table.XXPA_TS_HEADER_APPROVALS.sql
setlocal fdm=syntax
setlocal fde=0
setlocal fmr={{{,}}}
setlocal fdi=#
setlocal fdl=0
setlocal fml=1
setlocal fdn=20
setlocal nofen
let s:l = 33 - ((32 * winheight(0) + 42) / 85)
if s:l < 1 | let s:l = 1 | endif
keepjumps exe s:l
normal! zt
keepjumps 33
normal! 030|
lcd /mnt/d/Development/xxpa_ts
tabnext 1
badd +1 /mnt/d/Development/xxpa_ts/ebs_xxpa/sql/c4042.table.XXPA_TS_HEADER_APPROVALS.sql
badd +0 /mnt/d/Development/xxpa_ts/ebs_xxpa/sql/c4240.package.XXPA_TS_HEADER_PKG.sql
if exists('s:wipebuf') && len(win_findbuf(s:wipebuf)) == 0 && getbufvar(s:wipebuf, '&buftype') isnot# 'terminal'
  silent exe 'bwipe ' . s:wipebuf
endif
unlet! s:wipebuf
set winheight=1 winwidth=20 shortmess=filnxtToOFI
let s:sx = expand("<sfile>:p:r")."x.vim"
if filereadable(s:sx)
  exe "source " . fnameescape(s:sx)
endif
let &g:so = s:so_save | let &g:siso = s:siso_save
set hlsearch
doautoall SessionLoadPost
unlet SessionLoad
" vim: set ft=vim :
